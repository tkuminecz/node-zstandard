{
  "name": "zstandard",
  "version": "0.1.0",
  "description": "zstandard bindings for Node",
  "author": "Tim Kuminecz <tkuminecz@gmail.com>",
  "license": "MIT",
  "keywords": [
    "zstd",
    "zstandard",
    "compress",
    "compression",
    "node"
  ],
  "homepage": "https://github.com/tkuminecz/node-zstandard#readme",
  "bugs": {
    "url": "https://github.com/tkuminecz/node-zstandard/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkuminecz/node-zstandard.git"
  },
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "postinstall": "./node_modules/.bin/node-gyp rebuild -v",
    "preinstall": "npm run zstd",
    "test": "node ./test/zstd.js",
    "zstd": "npm run zstd:install && npm run zstd:build && npm run zstd:copy",
    "zstd:install": "git submodule init && git submodule update",
    "zstd:build": "make -C deps/zstd/lib",
    "zstd:copy": "mkdir -p build/lib && cp deps/zstd/lib/libzstd.a build/lib/"
  },
  "devDependencies": {
    "node-gyp": "^3.4.0",
    "tape": "^4.6.0"
  }
}
